<h1>Wiki's by Topic</h1>

<div class="row">
  <div class="col-md-8">
    <% @topic.each do |topics| %>
     <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= link_to topics.name, topics %>
        </h4>
        <% end %>
      </div>
    </div>
  </div>
</div>


    
<%= link_to "Add Topic", new_topic_path, class:'btn btn-danger btn-lg active' %>