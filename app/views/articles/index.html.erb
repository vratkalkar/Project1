<h1>All Wiki's</h1>

<div class="row">
  <div class="col-md-8">
    <% @articles.each do |article| %>
     <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= link_to article.title, article %>
          <% end %>
        </h4>
      </div>
    </div>
  </div>
</div>

<%= link_to "View Topics", topics_path(@topic), class: 'btn btn-primary' %>
